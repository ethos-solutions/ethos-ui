{
  "name": "organisation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/organisation/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/organisation/tsconfig.app.json --noEmit"
      }
    }
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/organisation"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        },
        "stage": {
          "mode": "stage"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "organisation:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "organisation:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "organisation:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "maxWarnings": -1
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "organisation:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/organisation/jest.config.ts"
      }
    },
    "preview": {
      "dependsOn": ["build"],
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "organisation:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "organisation:build:development"
        },
        "production": {
          "buildTarget": "organisation:build:production"
        }
      }
    }
  }
}
